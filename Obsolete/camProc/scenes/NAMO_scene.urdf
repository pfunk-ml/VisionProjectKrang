<world name="NAMO_scene">

  <include filename="chair.urdf" model_name="chair"/>
  <include filename="table.urdf" model_name="table"/>
  <include filename="rodPad.urdf" model_name="cube"/>
  <include filename="goldenPad.urdf" model_name="golden"/>
  <include filename="bicolorRod.urdf" model_name="cam"/>

  <entity model="golden" name="origin">
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
  </entity>

  <entity model="cam" name="cam0">
    <origin xyz="-1.0 0.0 0.0" rpy="0 0 0"/>
  </entity>

  <entity model="cam" name="cam1">
    <origin xyz="1.0 0.0 0.0" rpy="0 0 0"/>
  </entity>

  <entity model="cam" name="cam2">
    <origin xyz="0.0 1.0 0.0" rpy="0 0 0"/>
  </entity>

  <entity model="cam" name="cam3">
    <origin xyz="0.0 1.0 0.0" rpy="0 0 0"/>
  </entity>


  <entity model="cube" name="chair">
    <origin xyz="-1.0 -1.0 0.0" rpy="0 0 0"/>
  </entity>

  <entity model="cube" name="table">
    <origin xyz="-1.0 1.0 0.0" rpy="0 0 0"/>
  </entity>

  <entity model="cube" name="robot">
    <origin xyz="1.0 -1.0 0.0" rpy="0 0 0"/>
  </entity>


</world>
